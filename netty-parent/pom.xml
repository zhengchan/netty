    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>io.netty</groupId>
                <artifactId>netty-all</artifactId>
                <version>${netty.versions}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

</project>